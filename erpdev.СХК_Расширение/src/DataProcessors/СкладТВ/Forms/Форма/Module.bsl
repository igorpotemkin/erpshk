						ТаблДокум.Область("R"+а+"C"+б).ЦветФона = WebЦвета.Синий; 
						Если Веб = Истина Тогда
							ТаблДокум.Область("R"+а+"C"+б).ЦветТекста = WebЦвета.Синий;
						КонецЕсли;
						
					Иначе
						ТаблДокум.Область("R"+а+"C"+б).ЦветФона = Новый Цвет(255,124,128);
						Если Веб = Истина Тогда
							ТаблДокум.Область("R"+а+"C"+б).ЦветТекста = Новый Цвет(255,124,128);
						КонецЕсли;
						
					КонецЕсли;
